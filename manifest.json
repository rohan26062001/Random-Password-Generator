{
    "manifest_version": 2,
    "name": "Password Generator",
    "version": "1.0.0",
    "description": "Password Generator generates a password between length 5-50 characters",
    "icons": {
        "128": "icon_128.png"
      },
    "browser_action":{
      "default_icon": "icon.png",
      "default_popup": "index.html"
    },
    "permissions": ["activeTab"]
  }